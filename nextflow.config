params.regex = "'(S+)'"
params.pValCutoff = "1e-5"
params.lengthCutoff = "1"
params.percentCutoff = "1"
params.outputType = "both"
params.blastProgram = "blastp"
params.blastVendor = "ncbi"
params.databaseFasta = "$baseDir/data/test.fasta"
params.adjustMatchLength = ""
params.blastParamsFile = "$baseDir/data/blastParams"
params.saveAllBlastFiles = "false"
params.saveGoodBlastFiles = "false"
params.doNotParse = "false"
params.doNotExitOnBlastFailure = "false"
params.printSimSeqsFile = "false"
params.seqFile = "$baseDir/data/asicProteome.fasta"
params.outputDir = "$launchDir/output"
params.blastParams = "$baseDir/data/blastParams"
params.dataFile = "blastSimilarity.out"
params.logFile = "blastSimilarity.log"
params.databaseType = "protein"
process.container = 'veupathdb/blastsimilarity:latest'
docker {
    enabled = true
}
